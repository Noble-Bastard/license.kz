{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "export NODE_OPTIONS=--openssl-legacy-provider && cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "export NODE_OPTIONS=--openssl-legacy-provider && cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
